{
  "name": "@santan/frontend",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "NODE_ENV=production vite build",
    "serve": "vite preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{css,less,sass,scss,html,md,graphql,gql,json,ts,tsx,js,jsx,yml,yaml}\"",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "clean": "rm -rf .output dist .turbo",
    "setup:preview-secret": "node scripts/create-preview-secret.js",
    "get:preview-secret": "node scripts/get-preview-secret.js",
    "cleanup:preview-secrets": "node scripts/cleanup-preview-secrets.js",
    "publish:preview-secret": "node scripts/publish-secret.js"
  },
  "dependencies": {
