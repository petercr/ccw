    "@vanilla-extract/css": "^1.17.5",
    "@vanilla-extract/css-utils": "^0.1.6",
    "@vanilla-extract/vite-plugin": "^5.1.3",
    "groq": "^4.20.3",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "react-slick": "^0.31.0",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^4.1.17",
